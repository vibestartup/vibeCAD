{
  "name": "@vibecad/kernel",
  "version": "0.0.1",
  "description": "WASM kernel bindings for vibeCAD - OpenCascade and SolveSpace",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src",
    "wasm"
  ],
  "scripts": {
    "build": "tsc --build",
    "dev": "tsc --build --watch",
    "clean": "rm -rf dist tsconfig.tsbuildinfo",
    "typecheck": "tsc --noEmit",
    "lint": "echo 'lint not implemented'"
  },
  "dependencies": {
    "@vibecad/core": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "typescript": "^5.0.0"
  },
  "sideEffects": false
}
